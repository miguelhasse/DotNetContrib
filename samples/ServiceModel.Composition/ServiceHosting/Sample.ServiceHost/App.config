<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="hasseware.services" type="Sample.Configuration.SectionGroup">
      <section name="hosting" type="Sample.Configuration.HostingSection"/>
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.diagnostics>
    <sources>
      <source name="System.ServiceModel" switchValue="All" propagateActivity="true">
        <listeners>
          <add name="XmlListener" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="XmlListener" type="System.Diagnostics.XmlWriterTraceListener" initializeData="trace.svclog"/>
    </sharedListeners>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="XmlListener"/>
      </listeners>
    </trace>
  </system.diagnostics>
  <hasseware.services>
    <hosting />
  </hasseware.services>
</configuration>